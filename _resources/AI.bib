	journal = {Microorganisms},
	author = {Toro-Ascuy, Daniela and Cárdenas, Juan P. and Zorondo-Rodríguez, Francisco and González, Damariz and Silva-Moreno, Evelyn and Puebla, Carlos and Nunez-Parra, Alexia and Reyes-Cerpa, Sebastián and Fuenzalida, Loreto F.},
