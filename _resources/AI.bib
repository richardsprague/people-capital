	keywords = {wearable, hrv, covid19},
	pages = {e26107},
	file = {Full Text:/Users/sprague/Zotero/storage/6D9L4TAL/Hirten et al. - 2021 - Use of Physiological Data From a Wearable Device t.pdf:application/pdf},
}

