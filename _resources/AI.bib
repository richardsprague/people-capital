	keywords = {methods},
	pages = {e00606--19},
	file = {Full Text:/Users/sprague/Zotero/storage/WNK5EWFD/Diener et al. - 2020 - MICOM Metagenome-Scale Modeling To Infer Metaboli.pdf:application/pdf},
